              "MaxFormula": "1000",
              "MinFormula": "100",
              "StepFormula": "1",
              "PropertyFormula": "500"
            },

            {
              "Name": "Focal Length",
              "Units": "M",
              "DescriptionFormula": "'Distance to target (debug)'",
              "GuiHint": "GuiSelectionMaxMin",
              "MaxFormula": "100000000",
              "MinFormula": "1",
              "StepFormula": "1",
              "PropertyFormula": "1000000"
            },
            {
              "Name": "Emissitivity",
              "Units": "%",
              "DescriptionFormula": "'How emissive radiators are (tech)'",
              "GuiHint": "GuiSelectionMaxMin",
              "MaxFormula": "1",
              "MinFormula": "0",
              "StepFormula": "0.1",
              "PropertyFormula": "0.5"
            },

            {
              "Name": "Radiator Volume",
              "Units": "L",
              "DescriptionFormula": "'Volume of the chamber'",
              "GuiHint": "GuiSelectionMaxMin",
              "MaxFormula": "PropertyValue('Chamber Volume')",
              "MinFormula": "1",
              "StepFormula": "10",
              "PropertyFormula": "5000"
            },
			{
              "Name": "Radiator Thickness",
              "Units": "m",
              "DescriptionFormula": "'How thick radiators are (debug)'",
              "GuiHint": "GuiTextDisplay",
              "PropertyFormula": "Pow(PropertyValue('Radiator Volume'), 1/4)"
            },
            {
              "Name": "Radiator area",
              "Units": "m^2",
              "DescriptionFormula": "'How much area radiators have (debug)'",
              "GuiHint": "GuiTextDisplay",
              "PropertyFormula": "PropertyValue('Radiator Volume') / PropertyValue('Radiator Thickness')"
            },
            {
              "Name": "Operating Temprature",
              "Units": "K",
              "DescriptionFormula": "'What temp the laser operates at'",
              "GuiHint": "GuiTextDisplay",
              "PropertyFormula": "Pow( PropertyValue('Thermal Output') / (PropertyValue('Emissitivity') * PropertyValue('Radiator area') * 0.00000005670374419), 1/4 )"
            },
            {
              "Name": "Input Light",
              "Units": "W",
              "DescriptionFormula": "'Energy out put of a laser pulse'",
              "GuiHint": "GuiTextDisplay",
              "PropertyFormula": "PropertyValue('Conversion Efficency') * PropertyValue('Power Input')"
            },
            {
              "Name": "Chamber Absorption",
              "Units": "%",
              "DescriptionFormula": "'What percentage of stored light in the chamber is lost as heat (tech)'",
              "GuiHint": "GuiTextDisplay",
              "PropertyFormula": "(PropertyValue('Input Light') / (PropertyValue('Chamber Volume') * PropertyValue('Target Wavelength')))"
            },
            {
              "Name": "Conversion Efficency",
              "Units": "%",
              "DescriptionFormula": "'What percentage of input energy gets converted into light (tech)'",
              "GuiHint": "GuiTextDisplay",
              "PropertyFormula": "Sqrt( PropertyValue('Target Wavelength') / 1000 )"
            },
            {
              "Name": "Lense Emission",
              "Units": "%",
              "DescriptionFormula": "'What percentage of stored light is fired (tech)'",
              "GuiHint": "GuiTextDisplay",
              "PropertyFormula": "Sqrt( PropertyValue('Duty Cycle') )"
            },
            {
              "Name": "Pulse Energy",
              "Units": "J",
              "DescriptionFormula": "'Energy output of a laser pulse'",
              "GuiHint": "GuiTextDisplay",
              "PropertyFormula": "PropertyValue('Lense Emission') * (PropertyValue('Input Light') - PropertyValue('Input Light') * Exp(PropertyValue('Chamber Absorption') * -1 * PropertyValue('Charge Period'))) / PropertyValue('Chamber Absorption')"
            },
            {
              "Name": "Thermal Output",
              "Units": "W",
              "DescriptionFormula": "'Waste Thermal heat output by laser'",
              "GuiHint": "GuiTextDisplay",
              "PropertyFormula": "(PropertyValue('Power Input') * PropertyValue('Charge Period') - PropertyValue('Pulse Energy')) / PropertyValue('Charge Period')"
            },
            {
              "Name": "Beam waist",
              "Units": "M",
              "DescriptionFormula": "'Width of beam at focal length (debug)'",
              "GuiHint": "GuiTextDisplay",
              "PropertyFormula": " PropertyValue('Operating Temprature') * PropertyValue('Operating Temprature') * PropertyValue('Focal Length') * 0.000000001 * PropertyValue('Target Wavelength') * ( 1 / PropertyValue('Lens Diameter') ) "
            },
            {
              "Name": "genericWpndbargs",
              "DescriptionFormula": "'int magSize, int reloadPerSec, int amountPerShot, int minShotsPerfire",
              "GuiHint": "None",
              "PropertyFormula": "AtbConstrArgs(PropertyValue('Charge Period') * PropertyValue('Power Input'), PropertyValue('Power Input'), PropertyValue('Pulse Energy'), 1)",
              "AttributeType": "Pulsar4X.ECSLib.GenericWeaponAtb"
            },
            {
              "Name": "dbargs",
