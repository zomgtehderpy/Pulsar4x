            },
            {
              "Name": "Power Input",
              "Units": "W",
              "DescriptionFormula": "'How much power the laser consumes'",
              "GuiHint": "GuiSelectionMaxMin",
              "MaxFormula": "(PropertyValue('Chamber Volume') * PropertyValue('Target Wavelength') / PropertyValue('Conversion Efficency'))",
